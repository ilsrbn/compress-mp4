{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "enables the default permissions",
	"windows": ["main"],
	"permissions": [
		"path:default",
		"event:default",
		"window:default",
		"webview:default",
		"app:default",
		"resources:default",
		"image:default",
		"menu:default",
		"tray:default",
		"dialog:default",
		"dialog:allow-open",
		{
			"identifier": "shell:allow-execute",
			"allow": [
				{
					"args": [
						"-i",
						{
							"validator": "\\S+"
						},
						"-vcodec",
						{
							"validator": "\\S+"
						},

						{
							"validator": "\\S+"
						},
						{
							"validator": "\\S+"
						},
						{
							"validator": "\\S+"
						}
					],
					"cmd": "",
					"name": "binaries/ffmpeg",
					"sidecar": true
				}
			]
		},
		"shell:allow-open",
		"shell:default"
	]
}
